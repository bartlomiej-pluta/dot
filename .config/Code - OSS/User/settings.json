{
    "editor.minimap.enabled": false,
    "editor.fontFamily": "'Fira Code', 'Cascadia Code', 'FontAwesome', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.tabSize": 2,
    "git.autofetch": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.confirmDelete": false,
    "workbench.colorTheme": "Ayu Light Bordered",
    "indentRainbow.colors": [
        "rgba(255,255,64,0.05)",
        "rgba(127,255,127,0.05)",
        "rgba(255,127,255,0.05)",
        "rgba(79,236,236,0.05)"
    ],
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.defaultDateFormat": "dddd DD.MM.YYYY, HH:mm",
    "gitlens.defaultDateShortFormat": "DD.MM.YYYY"
}