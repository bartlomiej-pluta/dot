[Unit]
Description=Jupyter Notebook

[Service]
Type=simple
PIDFile=/run/jupyter.pid
ExecStart=/home/{{ yadm.user }}/.local/bin/jupyter notebook --config /home/{{ yadm.user }}/.jupyter/jupyter_notebook_config.py
WorkingDirectory=/home/{{ yadm.user }}/Dokumenty/Jupyter
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target
